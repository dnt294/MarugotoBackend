<p id="notice"><%= notice %></p>

<h1>New Word Adj Forms</h1>

<table>
  <thead>
    <tr>
      <th>Adj type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @new_word_adj_forms.each do |new_word_adj_form| %>
      <tr>
        <td><%= new_word_adj_form.adj_type %></td>
        <td><%= link_to 'Show', new_word_adj_form %></td>
        <td><%= link_to 'Edit', edit_new_word_adj_form_path(new_word_adj_form) %></td>
        <td><%= link_to 'Destroy', new_word_adj_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New New Word Adj Form', new_new_word_adj_form_path %>
